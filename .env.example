# Factory Configuration
FACTORY_MODEL=opencode/glm-4.7-free
FACTORY_PLANNING_CYCLES=3
FACTORY_VERIFICATION_CYCLES=3
FACTORY_WORKER_ITERATIONS=10
FACTORY_TIMEOUT=3600

# Logging Configuration (optional)
# FACTORY_LOG_FILE=factory.log  # Uncomment to enable file logging
FACTORY_LOG_LEVEL=info          # debug | info | warn | error

# Custom LLM Endpoint (optional)
# For self-hosted models like Nemotron, vLLM, Ollama
OPENAI_BASE_URL=
OPENAI_API_KEY=

# Cost Control (optional)
# Set maximum cost in USD, factory will stop if exceeded
FACTORY_MAX_COST=

